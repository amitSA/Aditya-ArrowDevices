import java.awt.BorderLayout;

import javax.swing.JButton;
import javax.swing.JPanel;
import javax.swing.JTabbedPane;


public class MessagePanel extends JPanel
{
	
	public MessagePanel(){
		super();
		
		JPanel supPanel = new JPanel(new BorderLayout());
		JTabbedPane tabs = new JTabbedPane();
		
		JPanel genPanel = new JPanel();genPanel.add(new JButton("sdfsdfsdf"));
		JPanel solvePanel = new JPanel();genPanel.add(new JButton("dsfsdfsdfdsf"));
		tabs.addTab("Generate",genPanel);
		tabs.addTab("Solve",solvePanel);
		
		
		supPanel.add(tabs,BorderLayout.CENTER);
		tabbedPane.setTabLayoutPolicy(JTabbedPane.SCROLL_TAB_LAYOUT);
		supPanel.setVisible(true);
	}
	
}
